{
  "require": {
    "adigital/cookie-consent-banner": "1.1.6",
    "chasegiunta/scss": "^1.0",
    "craftcms/cms": "3.3.11",
    "dolphiq/iconpicker": "^1.0",
    "vlucas/phpdotenv": "^3.4.0",
    "scssphp/scssphp": "1.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
